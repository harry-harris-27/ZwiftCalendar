<?xml version="1.0" encoding="utf-8" ?>
<Shell xmlns="http://xamarin.com/schemas/2014/forms"
       xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
       x:Class="Zwift.Calendar.Mobile.Views.Shell.ShellView"
       xmlns:cal="clr-namespace:Caliburn.Micro.Xamarin.Forms;assembly=Caliburn.Micro.Platform.Xamarin.Forms"
       Visual="Material"
       FlyoutBehavior="Disabled">
    <TabBar>
        <Tab Title="{Binding Calendar.DisplayName, Mode=OneWay}">
            <ShellContent cal:View.Model="{Binding Calendar, Mode=OneWay}" />
        </Tab>
    </TabBar>
</Shell>